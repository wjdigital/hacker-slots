ida.innerText = \"Iniciando an\u00e1lise...\";\n        setTimeout(function(){\n            saida.innerText = \"Encontrando padr\u00f5es...\";\n        }, 3000)\n        setTimeout(function(){\n            saida.innerText = \"Padr\u00e3o encontrado!\";\n        }, 8000)\n        setTimeout(function(){\n            sinais();\n        }, 10000)\n        \n    }\n    \n\n\n    \/\/ Adiciona o evento de clique ao bot\u00e3o\n    botao.addEventListener('click', function() {\n      \/\/ L\u00f3gica a ser executada quando o bot\u00e3o for clicado\n      console.log('O bot\u00e3o foi clicado!');\n      \/\/ Outras a\u00e7\u00f5es que voc\u00ea deseja realizar...\n      animacao();\n    });\n<\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-4a1ae889 elementor-section-full_width elementor-section-height-full elementor-section-items-top elementor-section-stretched elementor-section-height-default\" data-id=\"4a1ae889\" data-element_type=\"section\" data-settings=\"{&quot;stretch_section&quot;:&quot;section-stretched&quot;}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-3f710d29\" data-id=\"3f710d29\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<div class=\"elementor-element elementor-element-6fa2cef0 elementor-widget elementor-widget-html\" data-id=\"6fa2cef0\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<iframe style=\"height: 600px; border-radius: 25px; box-shadow: 0 0 31px -4px black;  border-width: 1px;\" src=\"https:\/\/eskybet.com\/casino\/c\/play\/REAL\/1301\/pragmatic\"><\/iframe>\r\n\r\n\r\n\t\t<\/div>\n\t\t\